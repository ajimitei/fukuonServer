<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <!-- フォーカス時 / focused -->
  <item
    android:state_focused="true"
    android:state_enabled="true"
    android:state_pressed="false">
      <color android:color="#66000000" />
  </item>
  
  <!-- 通常状態 / normal -->
  <item >
      <color android:color="#00FFFFFF" />
  </item>

</selector>
