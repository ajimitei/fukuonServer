package jp.tf_web.fukuon.network;

import java.io.Serializable;

/* リクエストの基本オブジェクト
 * 
 */
@SuppressWarnings("serial")
public class Request implements Serializable {

	public Request(){
		
	}

	public Request(REQUEST_METHOD_TYPE methodType){
		this.setMethodType(methodType);
		this.setOs(SmartDriveConfig.API_CLIENT_OS);
		this.setVersion(SmartDriveConfig.API_CLIENT_VERSION);
	}

	public Request(REQUEST_METHOD_TYPE methodType,String os,String version,User user){
		this(methodType,os,version);
		this.setUser(user);
	}
	
	public Response doInBackground() {
		return null;
	}
	
	public REQUEST_METHOD_TYPE getMethodType() {
		return methodType;
	}
	public void setMethodType(REQUEST_METHOD_TYPE methodType) {
		this.methodType = methodType;
	}
	public String getOs() {
		return os;
	}
	public void setOs(String os) {
		this.os = os;
	}
	public String getVersion() {
		return version;
	}
	public void setVersion(String version) {
		this.version = version;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}
}
