package jp.tf_web.fukuon.network.model;

@SuppressWarnings("serial")
public class UserRequest extends Request {
	private int id;
	private String name;
	private String photo;
	private String showName;
	private String message;
	
	public UserRequest(int id,String name,String photo,String showName,String message){
		this.setId( id );
		this.setName( name );
		this.setPhoto( photo );
		this.setShowName(showName);
		this.setMessage(message);
	}
	
	@Override
	public Response doInBackground() {
		String url = "http://192.168.1.178:3000/user";
		return null;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhoto() {
		return photo;
	}

	public void setPhoto(String photo) {
		this.photo = photo;
	}

	public String getShowName() {
		return showName;
	}

	public void setShowName(String showName) {
		this.showName = showName;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}
}
